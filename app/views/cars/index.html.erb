<head>
 <meta name="turbolinks-visit-control" content="reload">
</head>
<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Make</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <% if car.user_id = current_user.id %>
            <td><%= car.year %></td>
            <td><%= car.make %></td>
            <td><%= car.model %></td>
        <% end %>    
      </tr>
    <% end %>
  </tbody>
</table>