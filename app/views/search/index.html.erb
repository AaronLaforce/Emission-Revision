<title>Search Results</title>
<table class="container">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <% @users.each do |user| %>
    <% if user.id != current_user.id%>
      <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= link_to "View Their Fuel Logs", fuel_logs_path(id: user.id), class: "btn btn-primary"  %></td>
      </tr>
    <% end %>
   <% end %>
  </tbody>
</table>